<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/main_bg_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0_5"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@color/main_line_color"></View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">
        <ImageView
            android:id="@+id/me_about_logo"
            android:layout_width="@dimen/dp_90"
            android:layout_height="@dimen/dp_90"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_centerHorizontal="true"
            android:background="@drawable/login_logo"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/main_hit_text_color"
            android:layout_below="@+id/me_about_logo"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_marginBottom="@dimen/dp_20"
            android:textSize="@dimen/sp_16"
            android:text="V3.0.0"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0_5"
        android:background="@color/main_line_color"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@color/white"
        android:orientation="vertical">
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            android:background="@color/main_line_color"></View>
        <RelativeLayout
            android:id="@+id/me_about_cooperation"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40">
            <TextView
                android:id="@+id/me_home_descent"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="商务合作"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/main_title_text_color"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp" />

            <ImageView
                android:id="@+id/me_name_descent_right_arrow"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/me_button_xiayiye_default" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            android:background="@color/main_line_color"
            android:layout_marginRight="@dimen/dp_15"
            android:layout_marginLeft="@dimen/dp_15"></View>
        <RelativeLayout
            android:id="@+id/me_about_service_number"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="电话"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/main_title_text_color"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/me_home_money_picture"
                android:layout_marginLeft="15dp" />

            <TextView
                android:id="@+id/me_about_service_number_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/me_name_right_arrow"
                android:text="4001515517 "
                android:textColor="@color/main_assist_text_color"
                android:textSize="@dimen/sp_14" />
            <ImageView
                android:id="@+id/me_name_right_arrow"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/me_button_xiayiye_default" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            android:background="@color/main_line_color"
            android:layout_marginRight="@dimen/dp_15"
            android:layout_marginLeft="@dimen/dp_15"></View>
        <RelativeLayout
            android:id="@+id/me_about_customer_service"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="联系客服"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/main_title_text_color"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="QQ客服"
                android:textColor="@color/main_assist_text_color"
                android:layout_centerVertical="true"
                android:textSize="@dimen/sp_14"
                android:layout_toLeftOf="@+id/me_name_right_arrow2"/>

            <ImageView
                android:id="@+id/me_name_right_arrow2"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/me_button_xiayiye_default" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            android:background="@color/main_line_color"
            android:layout_marginRight="@dimen/dp_15"
            android:layout_marginLeft="@dimen/dp_15"></View>
        <RelativeLayout
            android:id="@+id/me_about_mailbox_number"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="邮箱"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/main_title_text_color"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/me_home_money_picture"
                android:layout_marginLeft="15dp" />

            <TextView
                android:id="@+id/me_about_mailbox_number_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/me_name_right_arrow1"
                android:text="kf@huaqinchi.com "
                android:textColor="@color/main_assist_text_color"
                android:textSize="@dimen/sp_14" />
            <ImageView
                android:id="@+id/me_name_right_arrow1"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/me_button_xiayiye_default" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            android:background="@color/main_line_color"></View>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0_5"
        android:layout_marginTop="@dimen/dp_15"
        android:background="@color/main_line_color"></View>
    <RelativeLayout
        android:id="@+id/me_setting_feedback"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        android:background="@color/white">


        <TextView
            android:id="@+id/me_home_gift"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="去评分"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/main_title_text_color"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/me_home_gift_picture"
            android:layout_marginLeft="15dp" />

        <ImageView
            android:id="@+id/me_name_gift_right_arrow"
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_30"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/me_button_xiayiye_default" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0_5"
        android:background="@color/main_line_color"></View>
</LinearLayout>