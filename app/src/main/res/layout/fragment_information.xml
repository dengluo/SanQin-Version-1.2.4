                        android:layout_height="@dimen/dp_10"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_20"
                        android:background="@drawable/new_icon_xiangshang_default" />
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_0_5"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@color/main_line_color"></View>
                <RelativeLayout
                    android:id="@+id/pager_third_rangking_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_50"
                    android:gravity="center_vertical">

                    <ImageView
                        android:id="@+id/pager_third_rangking_image"
                        android:layout_width="@dimen/dp_40"
                        android:layout_height="@dimen/dp_40"
                        android:layout_marginLeft="@dimen/dp_7"
                        android:layout_marginTop="@dimen/dp_5" />

                    <TextView
                        android:id="@+id/pager_third_rangking_content"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dp_7"
                        android:layout_toRightOf="@+id/pager_third_rangking_image"
                        android:gravity="center_vertical"
                        android:text="中华招式宗亲会"
                        android:textColor="@color/main_link_text_color"
                        android:textSize="@dimen/sp_14" />

                    <TextView
                        android:id="@+id/pager_third_rangking_money"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginRight="@dimen/dp_7"
                        android:layout_toLeftOf="@+id/pager_third_rangking_up"
                        android:gravity="center_vertical"
                        android:text="18万元"
                        android:textSize="@dimen/sp_14" />

                    <ImageView
                        android:id="@+id/pager_third_rangking_up"
                        android:layout_width="@dimen/dp_10"
                        android:layout_height="@dimen/dp_10"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_20"
                        android:background="@drawable/new_icon_xiangshang_default" />
                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_0_5"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@color/main_line_color"></View>
                <RelativeLayout
                    android:id="@+id/pager_fourth_rangking_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_50"
                    android:gravity="center_vertical">

                    <ImageView
                        android:id="@+id/pager_fourth_rangking_image"
                        android:layout_width="@dimen/dp_40"
                        android:layout_height="@dimen/dp_40"
                        android:layout_marginLeft="@dimen/dp_7"
                        android:layout_marginTop="@dimen/dp_5" />

                    <TextView
                        android:id="@+id/pager_fourth_rangking_content"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dp_7"
                        android:layout_toRightOf="@+id/pager_fourth_rangking_image"
                        android:gravity="center_vertical"
                        android:text="中华招式宗亲会"
                        android:textColor="@color/main_link_text_color"
                        android:textSize="@dimen/sp_14" />

                    <TextView
                        android:id="@+id/pager_fourth_rangking_money"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginRight="@dimen/dp_7"
                        android:layout_toLeftOf="@+id/pager_fourth_rangking_up"
                        android:gravity="center_vertical"
                        android:text="18万元"
                        android:textSize="@dimen/sp_14" />

                    <ImageView
                        android:id="@+id/pager_fourth_rangking_up"
                        android:layout_width="@dimen/dp_10"
                        android:layout_height="@dimen/dp_10"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_20"
                        android:background="@drawable/new_icon_xiangshang_default" />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <FrameLayout
        android:id="@+id/information_page_layout"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
