<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/dp_15">
        <include
            android:id="@+id/team_system_invite_in"
            layout="@layout/nim_advanced_team_info_divider_item"
            />

        <include
            android:id="@+id/team_system_invite_in_val"
            layout="@layout/nim_advanced_team_info_divider_item"/>
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginBottom="@dimen/dp_15">
        <include
            android:id="@+id/team_system_invite_in_val_jur"
            layout="@layout/nim_advanced_team_info_divider_item"
            />

        <include
            android:id="@+id/team_system_invite_in_edit_jur"
            layout="@layout/nim_advanced_team_info_divider_item"/>
    </LinearLayout>
    <include
        android:id="@+id/team_system_make_over_team"
        layout="@layout/nim_advanced_team_info_divider_item"/>

</LinearLayout>
